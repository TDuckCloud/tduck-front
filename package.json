  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build-dev": "vue-cli-service build --mode development --dest dist-dev",
    "preview": "node build/index.js --preview",
    "build": "vue-cli-service build",
