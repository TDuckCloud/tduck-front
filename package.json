    "eslint": "^7.12.1",
    "eslint-plugin-vue": "^7.1.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "plop": "^2.7.4",
    "sass": "~1.32.1",
    "sass-loader": "^10.0.4",
    "sass-resources-loader": "^2.1.1",
